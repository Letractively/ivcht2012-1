<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>DatabaseController</servlet-name>
        <servlet-class>edu.sstu.ivcht.term2012.controller.DatabaseController</servlet-class>
    </servlet>
    <servlet>
    <servlet-name>ListBrandController</servlet-name>
    <servlet-class>edu.sstu.ivcht.term2012.controller.ListBrandController</servlet-class>
</servlet>
    <servlet>
        <servlet-name>ListAutoController</servlet-name>
        <servlet-class>edu.sstu.ivcht.term2012.controller.ListAutoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListTypesController</servlet-name>
        <servlet-class>edu.sstu.ivcht.term2012.controller.ListTypesController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DatabaseController</servlet-name>
        <url-pattern>/createdatabase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListBrandController</servlet-name>
        <url-pattern>/listbrand</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAutoController</servlet-name>
        <url-pattern>/listauto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListTypesController</servlet-name>
        <url-pattern>/listtypes</url-pattern>
    </servlet-mapping>
</web-app>
